{
  "version": 2,
  "type": "module",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/query",
      "dest": "/server.js"
    }
  ],
  "env": {
    "XATA_API_KEY": "@xata_api_key",
    "XATA_DATABASE_URL": "@xata_database_url"
  }
}
